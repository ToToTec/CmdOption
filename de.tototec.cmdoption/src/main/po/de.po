#: de/tototec/cmdoption/handler/BooleanFieldHandler.java:87
#: de/tototec/cmdoption/handler/BooleanHandler.java:94
#, java-format
msgid "Could not parse argument \"{0}\" as boolean parameter."
msgstr "Argument {0} konnte nicht als Wahrheitswert gelesen werden."

#: de/tototec/cmdoption/handler/BooleanFieldHandler.java:96
#: de/tototec/cmdoption/handler/BooleanHandler.java:108
#: de/tototec/cmdoption/handler/IntegerHandler.java:61
#, java-format
msgid "Could not apply argument \"{0}\"."
msgstr "Parameter {0} konnte nicht angewendet werden."

#: de/tototec/cmdoption/handler/IntegerHandler.java:47
#, java-format
msgid "Could not read integer value \"{0}\"."
msgstr "Konnte den Integer-Wert \"{0}\" nicht lesen."

#: de/tototec/cmdoption/handler/PutIntoMapHandler.java:28
#, java-format
msgid "Could not apply parameters {0} to field {1}"
msgstr "Parameter {0} konnte zu Feld {1} zugewiesen werden"

#: de/tototec/cmdoption/handler/StringFieldHandler.java:35
#, java-format
msgid "Could not apply parameters: {0} to field {1}"
msgstr "Parameter {0} konnte nicht auf Feld {1} angewendet werden"

#: de/tototec/cmdoption/handler/StringMethodHandler.java:39
#, java-format
msgid "Could not apply parameters: {0} to method {1}"
msgstr "Parameter {0} konnte nicht auf Methode {1} angewendet werden"

#: de/tototec/cmdoption/handler/UrlHandler.java:41
#, java-format
msgid "Invalid url: \"{0}\""
msgstr "Ungültige URL: \"{0}\""

#: de/tototec/cmdoption/handler/UrlHandler.java:46
#, java-format
msgid "Could not apply parameters: {0} to field/method {1}"
msgstr "Parameter {0} konnte nicht auf Feld/Methode {1} angewendet werden"

